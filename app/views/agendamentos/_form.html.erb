				<div class="col-md-10">
					<select class="form-control" name="medico" required>
						<option>--- Selecione um médico --</option>
						<option>Alessandro Costa</option>
						<option>Marcelo Souza</option>
						<option>Pedro Alves</option>
						<option>Renato Alcântara</option>
					</select>
				</div>
			</div>
			<div class="form-group">
				<div class="col-md-10 col-md-offset-2"> 

					<button type="submit" class="btn btn-primary">Avançar</button>
				</div> 
			</div>
		</form>
	</div>
