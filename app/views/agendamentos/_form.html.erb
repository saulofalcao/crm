	<% content_for :head do %>
	<script type="text/javascript">
		<%= render partial: "calendar_jquery" %>
	</script>
	<% end %>

	<div class="container">
		<form method="get" class="form-horizontal" role="form" action="/agendamentos/seleciona_horarios">
			<%= hidden_field_tag :authenticiy_token, form_authenticity_token %>
